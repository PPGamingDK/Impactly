<Window x:Class="Impactly_0._1.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Impactly_0._1.Views"
        mc:Ignorable="d"
        Title="RegisterPage" Height="450" Width="800">
    <Grid>
        <Frame x:Name="RegisterFrame" NavigationUIVisibility="Hidden"/>
        <TextBox HorizontalAlignment="Center" Name="EmailTBX" Margin="0,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox HorizontalAlignment="Center" PasswordChanged="PasswordBox_PasswordChanged"  VerticalAlignment="Top" Width="120" Margin="0,245,0,0"/>
        <Label Content="Email" HorizontalAlignment="Center" Margin="0,160,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.284,-0.592"/>
        <Label Content="Password" HorizontalAlignment="Center" Margin="0,220,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.284,-0.592"/>
        <Button Content="Login" Click="LoginWindow_Click"  HorizontalAlignment="Left" Margin="427,294,0,0" VerticalAlignment="Top" />
        <Button Content="Register" HorizontalAlignment="Left" Margin="340,294,0,0" VerticalAlignment="Top" Click="RegisterWindow_Click"/>
    </Grid>
</Window>
